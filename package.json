{
  "name": "planning-poker-trpc",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:* --print-label",
    "dev:server": "tsx watch src/server",
    "dev2:wss": "wait-port 3030 && tsx watch src/server/wsServer.ts",
    "dev:client": "wait-port 3030 && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start:server": "node dist/server/index.js",
    "start:client": "wait-port 3030 && node dist/client/index.js",
    "start": "run-p start:* --print-label",
    "test-dev": "start-server-and-test 'tsx src/server' http-get://localhost:3030 'tsx src/web'",
    "test-start": "start-server-and-test 'node dist/server' http-get://localhost:3030 'node dist/client'"
  },
  "dependencies": {
    "@fastify/mysql": "^4.1.0",
    "@fastify/websocket": "^7.1.1",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-router": "^0.0.1-alpha.0",
    "@trpc/client": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "abort-controller": "^3.0.0",
    "fastify": "^4.10.2",
    "mysql2": "^2.3.3",
    "node-fetch": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-minimal-pie-chart": "^8.4.0",
    "superjson": "^1.7.4",
    "tslib": "^2.1.0",
    "ws": "^8.11.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.20",
    "@types/node-fetch": "^2.5.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-react": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.1",
    "start-server-and-test": "^1.12.0",
    "tsx": "^3.9.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "wait-port": "^1.0.1"
  },
  "publishConfig": {
    "access": "restricted"
  }
