{
  "name": "planning-poker-trpc",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "tsc": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "prettier": "prettier \"**/*.{js,ts,tsx,md,html,css,scss,json}\" --check --loglevel warn",
    "prettier:fix": "prettier \"**/*.{js,ts,tsx,md,html,css,scss,json}\" --write --loglevel warn",
    "fix": "npm run lint:fix && npm run prettier:fix",
    "dev": "NODE_ENV=development run-p dev:* --print-label",
    "dev:server": "NODE_ENV=development tsx watch src/server",
    "dev2:wss": "wait-port 3030 && tsx watch src/server/wsServer.ts",
    "dev:client": "vite",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "preview": "vite preview",
    "start:server": "node dist/src/server/index.js",
    "start:client": "vite preview",
    "start": "run-p start:* --print-label",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/websocket": "^7.1.2",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-router": "^0.0.1-beta.83",
    "@trpc/client": "^10.13.2",
    "@trpc/react-query": "^10.13.2",
    "@trpc/server": "^10.13.2",
    "abort-controller": "^3.0.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.13.0",
    "mysql2": "^3.1.2",
    "node-fetch": "^3.3.0",
    "preact": "^10.13.0",
    "react-minimal-pie-chart": "^8.4.0",
    "sass": "^1.58.3",
    "superjson": "^1.12.2",
    "tslib": "^2.5.0",
    "ws": "^8.12.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/node": "^18.14.4",
    "@types/node-fetch": "^2.6.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.8.4",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "wait-port": "^1.0.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
