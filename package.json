{
  "name": "planning-poker-trpc",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "tsc": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "prettier": "prettier \"**/*.{js,ts,tsx,md,html,css,scss,json}\" --check --loglevel warn",
    "prettier:fix": "prettier \"**/*.{js,ts,tsx,md,html,css,scss,json}\" --write --loglevel warn",
    "fix": "npm run lint:fix && npm run prettier:fix",
    "dev": "NODE_ENV=development run-p dev:* --print-label",
    "dev:server": "NODE_ENV=development tsx watch src/server",
    "dev2:wss": "wait-port 3030 && tsx watch src/server/wsServer.ts",
    "dev:client": "wait-port 3030 && vite",
    "build:client": "VITE_APP_PORT=4173 vite build",
    "build:server": "tsc --project tsconfig.server.json && cp ./server.key ./dist/src/server && cp ./server.crt ./dist/src/server",
    "build": "run-s build:*",
    "preview": "vite preview",
    "start:server": "node dist/src/server/index.js",
    "start:client": "wait-port 0.0.0.0:3030 && VITE_APP_PORT=4173 vite preview",
    "start": "run-p start:* --print-label",
    "test-dev": "start-server-and-test 'tsx src/server' http-get://localhost:3030 'tsx src/web'",
    "test-start": "start-server-and-test 'node dist/server' http-get://localhost:3030 'node dist/client'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/mysql": "^4.1.0",
    "@fastify/websocket": "^7.1.2",
    "@tanstack/react-query": "^4.22.0",
    "@tanstack/react-router": "^0.0.1-beta.53",
    "@trpc/client": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "abort-controller": "^3.0.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "mysql2": "^3.0.1",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-minimal-pie-chart": "^8.4.0",
    "superjson": "^1.12.2",
    "tslib": "^2.4.1",
    "ws": "^8.12.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.3",
    "start-server-and-test": "^1.15.2",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "wait-port": "^1.0.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
